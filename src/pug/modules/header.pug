mixin nav(nav_array)
    ul.nav
        each i in nav_array
            +nav_item(i.url, i.name, i.submenu)
mixin nav_item(url, name, submenu)
    if(submenu)
            li.nav-item.item-with-submenu
                +submenu(submenu, name)
                a(href=url)= name
                    span.submenu-indicator >
                
    else
        li.nav-item
            a(href= url)= name

mixin submenu(nav_array, title)
    ul.submenu   
        h3= title
        each i in nav_array
            +submenu_item(i.url, i.name, i.submenu)

mixin submenu_item(url, name, submenu)
    if(submenu)
        li.nav-item
            +submenu(submenu, name)
            a(href= url)= name
            span.submenu-indicator >
    else
        li.nav-item
            a(href= url)= name
        
.nav-container
    .navs
        +nav(nav_main)
        +nav(nav_extra)
        +nav(nav_more)
